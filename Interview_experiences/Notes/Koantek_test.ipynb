    "import math\n",
    "large_num = 11\n",
    "\n",
    "def check(number):\n",
    "    if large_num<1:\n",
    "        return False\n",
    "    for i in range(2, int(math.sqrt(number))+1):\n",
    "        if large_num % i == 0:\n",
    "            return False\n",
    "    return True\n",
    "\n",
    "prime = check(large_num)  \n",
    "prime\n"
   ]
